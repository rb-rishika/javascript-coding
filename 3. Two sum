/**
 * @param {number[]} nums
 * @param {number} target
 * @return {number[]}
 */
var twoSum = function(nums, target) {
    const mapObject={}
    for(let i=0; i<nums.length; i++) {
        const num=nums[i]
        const missingElement= target-num
        
        if(missingElement in mapObject){
            return [i, mapObject[missingElement]]
        }
        mapObject[num]=i
    }
};
